{
  "name": "bio",
  "homepage": "http://www.ahmet.se/",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "eslint-plugin-flowtype": "^2.50.0",
    "isomorphic-fetch": "^2.2.1",
    "node-sass": "^4.9.4",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts": "2.0.3"
  },
  "parser": "babel-eslint",
  "rules": {
    "strict": 0
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint src",
    "flow": "flow",
    "pretty": "prettier --write --tab-width 2 \"src/**/*.js\"",
    "precommit": "lint-staged",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "text"
    ]
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run pretty",
      "git add"
    ]
  },
  "devDependencies": {
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.79.1",
    "gh-pages": "^1.2.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
